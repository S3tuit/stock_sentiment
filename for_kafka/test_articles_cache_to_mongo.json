{
    "name": "just_testing",
    "config": {
        "connection.uri": "mongodb://peppa:peppa@mongodb:27017",
        "database": "stock_test",
        "collection": "just_testing",
        "topics": "test.articles",
        "connector.class": "com.mongodb.kafka.connect.MongoSinkConnector",
        "tasks.max": 1,
        "key.converter": "org.apache.kafka.connect.storage.StringConverter",
        "key.converter.schemas.enable": "false",
        "value.converter": "io.confluent.connect.avro.AvroConverter",
        "value.converter.schema.registry.url": "http://schema-registry:8081",
        "errors.tolerance": "all",

        "post.processor.chain": "com.mongodb.kafka.connect.sink.processor.AllowListValueProjector", 
        "value.projection.type": "AllowList",
        "value.projection.list": "ticket,title,source",
        
        "document.id.strategy": "com.mongodb.kafka.connect.sink.processor.id.strategy.PartialValueStrategy",
        "document.id.strategy.partial.value.projection.list": "ticket,source",
        "document.id.strategy.partial.value.projection.type": "AllowList",
        "writemodel.strategy": "com.mongodb.kafka.connect.sink.writemodel.strategy.UpdateOneBusinessKeyTimestampStrategy"
    }
}
